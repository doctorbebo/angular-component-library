{
    "name": "storybook",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "yarn storybook",
        "build": "ng build",
        "storybook": "start-storybook -p 6006 -s ../../docs/,./assets,./node_modules/@fontsource/open-sans/files",
        "build-storybook": "build-storybook -s ../../docs/,./assets,./node_modules/@fontsource/open-sans/files",
        "lint": "eslint \"stories/**/**.ts\"",
        "lint:fix": "eslint \"stories/**/**.ts\" --fix",
        "prettier": "prettier \"**/**.{ts,json,css,scss,html}\" --write",
        "prettier:check": "prettier \"**/**.{ts,json,css,scss,html}\" --check",
        "deploy:feature": "set npm_config_yes=true && npx -p firebase-tools firebase deploy --non-interactive --token $FIREBASE_FEATURE_TOKEN"
    },
    "prettier": "@pxblue/prettier-config",
    "private": true,
    "dependencies": {
        "@angular/animations": "~10.1.4",
        "@angular/cdk": "^10.1.4",
