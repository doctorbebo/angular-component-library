        "@angular/core": "~10.1.4",
        "@angular/forms": "^10.1.4",
        "@angular/material": "^10.1.4",
        "@angular/platform-browser": "~10.1.4",
        "@angular/platform-browser-dynamic": "~10.1.4",
        "@pxblue/angular-themes": "^6.0.3",
        "@pxblue/colors": "^3.0.0",
        "@pxblue/icons": "^1.2.0",
        "@pxblue/icons-svg": "^1.2.0",
        "@pxblue/ng-progress-icons": "^1.2.1",
        "@pxblue/storybook-rtl-addon": "^1.0.0",
        "@pxblue/storybook-themes": "^1.1.0",
        "core-js": "^3.6.5",
        "node-sass": "^4.14.1",
        "rxjs": "~6.6.0",
        "tslib": "^2.0.0",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.1001.4",
        "@angular/cli": "~10.1.4",
        "@angular/compiler-cli": "~10.1.4",
        "@angular/language-service": "^10.1.4",
        "@babel/core": "^7.7.7",
        "@pxblue/eslint-config": "^2.0.1",
        "@pxblue/prettier-config": "^1.0.2",
        "@storybook/addon-a11y": "^5.3.18",
        "@storybook/addon-actions": "^5.3.19",
